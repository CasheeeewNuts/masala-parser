{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["dom", "es2015", "es2017"],
    "strict": true,
    "noEmit": false,
    "isolatedModules": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true
  }
}
