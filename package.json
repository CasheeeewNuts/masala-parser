    }
  ],
  "main": "src/index.js",
  "engines": {
    "node": ">= 5.7.0"    
  },
  "scripts": {
    "compile": "babel src --out-dir dist  --source-maps",
    "dev": "babel --watch src --out-dir dist",
    "lint": "eslint src/**/*.js",
    "test": "npm run compile && nodeunit dist/test/index.js",
    "cover": "rm -rf ./coverage && babel-node  ./node_modules/.bin/babel-istanbul cover --dir coverage/istanbul src/test/index.js",
    "coveralls": "cat ./coverage/istanbul/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "check": "npm run lint && npm run test && npm run cover && npm run coveralls",
    "concat": "npm run compile && browserify dist/lib/index.js -o dist/parsec.js"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-istanbul": "^0.8.0",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "~9.0.3",
    "coveralls": "~2.11.2",
    "eslint": "^1.2.1",
    "nodeunit": "~0.9.0"
  },
  "dependencies": {
  },
  "keywords": []
}
